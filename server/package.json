{
  "name": "loved-server",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run lint && rm -rf build && tsc --noEmitOnError && npm run permissions",
    "lint": "eslint --max-warnings 0 .",
    "permissions": "chmod +x ./build/index.js ./build/init-user.js ./build/bin/update-extra-tokens.js ./build/update-stale-beatmapsets.js",
    "start": "rm -rf build && exec tsc-watch --noClear --onSuccess \"sh -c 'npm run permissions && ./build/index.js'\""
  },
  "dependencies": {
    "async-lock": "^1.3.2",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-mysql-session": "^2.1.8",
    "express-session": "^1.17.3",
    "http-terminator": "^3.2.0",
    "loved-bridge": "file:../bridge",
    "mysql": "^2.18.1",
    "superagent": "^8.0.0"
  },
  "devDependencies": {
    "@types/async-lock": "^1.3.0",
    "@types/express": "^4.17.14",
    "@types/express-mysql-session": "^2.1.3",
    "@types/mysql": "^2.15.21",
    "@types/node": "^16.11.60",
    "@types/superagent": "^4.1.15",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-regex": "^1.10.0",
    "prettier": "^2.7.1",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.8.3"
  },
  "engines": {
    "node": ">=16"
  }
}
